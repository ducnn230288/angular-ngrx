{
  "name": "admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "ng build --configuration production && npm run format:build",
    "build:stag": "ng build --configuration staging && npm run format:build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"src/**/*.{ts,html,less}\"",
    "format:build": "prettier --write \"dist/**/*.html\" --print-width=999999",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "test-storybook": "test-storybook",
    "chromatic": "node --max_old_space_size=4096 && chromatic --exit-zero-on-changes"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@angular/service-worker": "^15.2.9",
    "@editorjs/code": "^2.8.0",
    "@editorjs/delimiter": "^1.3.0",
    "@editorjs/editorjs": "^2.27.2",
    "@editorjs/embed": "^2.5.3",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.1",
    "@editorjs/inline-code": "^1.4.0",
    "@editorjs/link": "^2.5.0",
    "@editorjs/list": "^1.8.0",
    "@editorjs/marker": "^1.3.0",
    "@editorjs/quote": "^2.5.0",
    "@editorjs/raw": "^2.4.0",
    "@editorjs/table": "^2.2.2",
    "@editorjs/underline": "^1.1.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "codex-notifier": "^1.1.2",
    "codex-tooltip": "^1.0.5",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.3",
    "glightbox": "^3.2.0",
    "gsap": "^3.11.5",
    "jquery": "^3.6.3",
    "line-awesome": "^1.3.0",
    "ng-zorro-antd": "^15.1.0",
    "ngx-color-picker": "^14.0.0",
    "ngx-echarts": "^15.0.3",
    "ngx-mask": "^15.1.5",
    "rxjs": "~7.8.0",
    "slug": "^8.2.2",
    "summernote": "^0.8.20",
    "sweetalert2": "^11.6.15",
    "swiper": "9.2.4",
    "tslib": "^2.4.1",
    "vanilla-lazyload": "^17.8.3",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.9",
    "@angular/cli": "~15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@babel/core": "^7.20.7",
    "@types/node": "^12.11.1",
    "@types/slug": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-loader": "^8.3.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
